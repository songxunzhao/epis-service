		<xsd:annotation>
			<xsd:documentation>X19 KALKULAATOR</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="EpisMessageType">
				<xsd:choice>
					<xsd:element name="Request" type="EpisX19RequestType"/>
					<xsd:element name="Response" type="EpisX19ResponseType"/>
				</xsd:choice>
			</xsd:extension>
