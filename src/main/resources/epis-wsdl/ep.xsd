					</xsd:attribute>
					<xsd:attribute name="Code" use="required">
						<xsd:simpleType>
							<xsd:annotation>
								<xsd:documentation>
			                                  Eesti puhul registrikood  
			                    </xsd:documentation>
							</xsd:annotation>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="11"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
					<xsd:attribute name="Name" use="required">
						<xsd:simpleType>
							<xsd:annotation>
								<xsd:documentation>
			                                Juriidilise isiku nimetus
			                    </xsd:documentation>
							</xsd:annotation>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="50"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
					<xsd:attribute name="E_MAIL" use="required">
						<xsd:simpleType>
							<xsd:annotation>
								<xsd:documentation>
			                                E-post.
			                    </xsd:documentation>
							</xsd:annotation>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="240"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
					<xsd:attribute name="Phone">
						<xsd:simpleType>
							<xsd:annotation>
								<xsd:documentation>
			                                Kontakttelefon
			                    </xsd:documentation>
							</xsd:annotation>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="15"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:complexType>
			</xsd:element>
			<xsd:choice>
				<xsd:element name="Address_ADS" type="AddressADSType" minOccurs="0" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>
                                                          AADRESS ADS
                                                  </xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="Address" type="AddressType" minOccurs="0" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation>
                                                          AADRESS
                                                  </xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
			<xsd:element name="Preferences" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>		
	Täidetakse ainult Eesti füüsilise isiku puhul, kellel on olemas pensionikonto
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="LanguagePreference" type="LangType" use="required">
						<xsd:annotation>
							<xsd:documentation>
		Keele eelistus
	</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="ExtractFlag" type="YesNoType" use="required">
						<xsd:annotation>
							<xsd:documentation>
		Väljavõtte soov Kas saata kord aasta pensionikonto väljavõte.
			                            Lubatud  väärtused:
			                            Y  - on soov
			                            N - mitte saata
	</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="ContactPreference" type="MailType" use="required">
						<xsd:annotation>
							<xsd:documentation>
			                            Lubatud E (e-mail), P (postiga)
			                </xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Bequeather" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>		
	Pärandaja andmed
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="PersonId" use="required">
						<xsd:simpleType>
							<xsd:annotation>
								<xsd:documentation>
			                                  Eesti isikukood
			                    </xsd:documentation>
							</xsd:annotation>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="11"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
					<xsd:attribute name="FirstName" use="required">
						<xsd:simpleType>
							<xsd:annotation>
								<xsd:documentation>
			                                  Pärandaja eesnimi
			                    </xsd:documentation>
							</xsd:annotation>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="50"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
					<xsd:attribute name="Name" use="required">
						<xsd:simpleType>
							<xsd:annotation>
								<xsd:documentation>
			                                Pärandaja perekonnanimi
			                    </xsd:documentation>
							</xsd:annotation>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="50"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Certificate" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>		
	Pärimistunnistuse andmed
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="CertificateNumber" use="required">
						<xsd:simpleType>
							<xsd:annotation>
								<xsd:documentation>
			                                  Pärimistunnistuse number
			                    </xsd:documentation>
							</xsd:annotation>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="10"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
					<xsd:attribute name="CertificateDate" type="xsd:date" use="required">
						<xsd:annotation>
							<xsd:documentation>
Pärimistunnistuse kuupäev
	</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="NumberOfHeirs" type="xsd:int" use="required">
						<xsd:annotation>
							<xsd:documentation>
Pärijate arv
	</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="CertificateNotary" use="required">
						<xsd:simpleType>
							<xsd:annotation>
								<xsd:documentation>
			                                  Notari ees- ja perekonnanimi
			                    </xsd:documentation>
							</xsd:annotation>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="30"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Agreement" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>		
	Pärandvara jagamise kokkuleppe andmed
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="AgreementDocumentNumber" use="required">
						<xsd:simpleType>
							<xsd:annotation>
								<xsd:documentation>
			                                  Pärandvara jagamise kokkuleppe number
			                    </xsd:documentation>
							</xsd:annotation>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="30"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
					<xsd:attribute name="AgreementDocumentDate" type="xsd:date" use="required">
						<xsd:annotation>
							<xsd:documentation>
Pärandvara jagamise kokkuleppe väljaandmise kuupäev
	</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="AgreementNotary" use="required">
						<xsd:simpleType>
							<xsd:annotation>
								<xsd:documentation>
			                                  Pärandvara jagamise kokkuleppe vormistanud notari ees- ja perekonnanimi
			                    </xsd:documentation>
							</xsd:annotation>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="60"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="ApplicationRows">
				<xsd:annotation>
					<xsd:documentation>
Avalduse read
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="ApplicationRow" minOccurs="0" maxOccurs="unbounded">
							<xsd:annotation>
								<xsd:documentation>
</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:attribute name="ISIN" use="required">
									<xsd:annotation>
										<xsd:documentation>
			Väärtpaberi ISIN, mille osakud pärandati
		</xsd:documentation>
									</xsd:annotation>
									<xsd:simpleType>
										<xsd:restriction base="xsd:string">
											<xsd:maxLength value="12"/>
										</xsd:restriction>
									</xsd:simpleType>
								</xsd:attribute>
								<xsd:attribute name="AmountTransfer" type="xsd:decimal">
									<xsd:annotation>
										<xsd:documentation>
			Ülekantavate osakute arv
		</xsd:documentation>
									</xsd:annotation>
								</xsd:attribute>
								<xsd:attribute name="AmountRedeem" type="xsd:decimal">
									<xsd:annotation>
										<xsd:documentation>
			Tagasivõetavate osakute arv
		</xsd:documentation>
									</xsd:annotation>
								</xsd:attribute>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="CashBalance" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>		
	Pärandvara jagamise kokkuleppe andmed
					</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="Cash" type="xsd:decimal">
						<xsd:annotation>
							<xsd:documentation>
Pärandatav rahaline summa
	</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="BankAccount" type="BankAccountType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
Pangakonto andmed.  Vajalik ainult siis, kui on tagasivõetavaid osakuid
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LegalRepresentative" type="LegalRepresentativeType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
SEADUSLIK_ESINDAJA
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Representative" type="RepresentativeType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>
ESINDAJA
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="DocumentDate" type="xsd:date" use="required"/>
		<xsd:attribute name="DocumentNumber">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:maxLength value="30"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="EpisX15ResponseType">
		<xsd:complexContent>
			<xsd:extension base="ApplicationResponseType">
				<xsd:sequence>
					<xsd:element name="Address" type="AddressType">
						<xsd:annotation>
							<xsd:documentation>
AADRESS
	</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="EpisX16Type">
		<xsd:annotation>
			<xsd:documentation>X16  TULUMAKSU ARVUTAMINE KINDLUSTUSSELTSILE</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="EpisMessageType">
				<xsd:choice>
					<xsd:element name="Request" type="EpisX16RequestType"/>
					<xsd:element name="Response" type="EpisX16ResponseType"/>
				</xsd:choice>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="EpisX16RequestType">
		<xsd:attribute name="RegistryCode" use="required">
			<xsd:annotation>
				<xsd:documentation>Päringu esitaja registrikood</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="RegistryCodeType"/>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="PersonId" use="required">
			<xsd:annotation>
				<xsd:documentation>Isikukood</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="PersoncodeType"/>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="Amount" use="required">
			<xsd:annotation>
				<xsd:documentation>Maksustamisele kuuluv summa</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:decimal">
					<xsd:minExclusive value="0"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="Currency" use="required">
			<xsd:annotation>
				<xsd:documentation>Vastavalt valuutade tabelile</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:length value="3"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="Residency">
			<xsd:annotation>
				<xsd:documentation>Residentriigi 3 –kohaline ISO3166 kood.   Täidetakse ainult siis, kui kindlustusseltsile on esitatud residentsustõend </xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:length value="3"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="ResidencyDocumentDate" type="xsd:date">
			<xsd:annotation>
				<xsd:documentation>Tulu saajat ja tema residentsust tõendava dokumendi väljaandmise kuupäev ( TSD Lisa 2 veerg 12 ) Täideakse ainult siis, kui kindlustusseltsile on esitatud residentsustõend</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="EpisX16ResponseType">
		<xsd:complexContent>
			<xsd:extension base="ResponseWithErrorInfoType">
				<xsd:sequence>
					<xsd:element name="IncomeTaxData" minOccurs="0">
						<xsd:complexType>
							<xsd:attribute name="RegistryCode" type="RegistryCodeType" use="required">
								<xsd:annotation>
									<xsd:documentation>Päringus olnud registrikood </xsd:documentation>
								</xsd:annotation>
							</xsd:attribute>
							<xsd:attribute name="PersonId" type="PersoncodeType" use="required">
								<xsd:annotation>
									<xsd:documentation>Päringus olnud isikukood</xsd:documentation>
								</xsd:annotation>
							</xsd:attribute>
							<xsd:attribute name="Amount" type="xsd:decimal" use="required">
								<xsd:annotation>
									<xsd:documentation>Päringus olnud summa</xsd:documentation>
								</xsd:annotation>
							</xsd:attribute>
							<xsd:attribute name="Usage_610" type="xsd:decimal" use="required">
								<xsd:annotation>
									<xsd:documentation>Summa, mille  ulatuses kasutatud  tulumaksu vabastust liik 610 osas</xsd:documentation>
								</xsd:annotation>
							</xsd:attribute>
							<xsd:attribute name="Usage_620" type="xsd:decimal" use="required">
								<xsd:annotation>
									<xsd:documentation>Summa, mille  ulatuses kasutatud  tulumaksu vabastust liik 620 osas</xsd:documentation>
								</xsd:annotation>
							</xsd:attribute>
							<xsd:attribute name="IncomeTax" type="xsd:decimal" use="required">
								<xsd:annotation>
									<xsd:documentation>Tulumaksu suurus</xsd:documentation>
								</xsd:annotation>
							</xsd:attribute>
							<xsd:attribute name="Currency" use="required">
								<xsd:annotation>
									<xsd:documentation>Vastavalt valuutade tabelile</xsd:documentation>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:length value="3"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:attribute>
							<xsd:attribute name="Rate" use="required">
								<xsd:annotation>
									<xsd:documentation>Millist tulumaksa määra kasutati</xsd:documentation>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:restriction base="xsd:decimal">
										<xsd:maxInclusive value="999.9"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:attribute>
							<xsd:attribute name="DeathDate" type="xsd:date">
								<xsd:annotation>
									<xsd:documentation>Surma kuupäev. Täidetakse kui isik on surnud</xsd:documentation>
								</xsd:annotation>
							</xsd:attribute>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="EpisX17Type">
		<xsd:annotation>
			<xsd:documentation>X17  NAV VÄÄRTUSE PÄRING</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="EpisMessageType">
				<xsd:choice>
					<xsd:element name="Request" type="EpisX17RequestType"/>
					<xsd:element name="Response" type="EpisX17ResponseType"/>
				</xsd:choice>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="EpisX17RequestType">
		<xsd:attribute name="ValidityDate" type="xsd:date" use="required">
			<xsd:annotation>
				<xsd:documentation>Kuupäev, millise seisuga  hindasid  soovitakse teada</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="ISIN" use="required">
			<xsd:annotation>
				<xsd:documentation>Väärtpaber, mille hindasid soovitakse teada</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:maxLength value="12"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="EpisX17ResponseType">
		<xsd:complexContent>
			<xsd:extension base="ResponseWithErrorInfoType">
				<xsd:attribute name="ValidityDate" type="xsd:date">
					<xsd:annotation>
						<xsd:documentation>Kuupäev, millise seisuga  hindasid  soovitakse teada</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="ISIN">
					<xsd:annotation>
						<xsd:documentation>Väärtpaber, mille hindasid soovitakse teada</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:maxLength value="12"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
				<xsd:attribute name="NAV" type="xsd:decimal">
					<xsd:annotation>
						<xsd:documentation>Puhasväärtus antud kuupäeval</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="SubscriptionPrice" type="xsd:decimal">
					<xsd:annotation>
						<xsd:documentation>Väljalaskehind</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="RedemptionPrice" type="xsd:decimal">
					<xsd:annotation>
						<xsd:documentation>Tagasivõtuhind</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="Currency">
					<xsd:annotation>
						<xsd:documentation>Valuuta kood</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:length value="3"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
				<xsd:attribute name="CheckDate" type="xsd:date">
					<xsd:annotation>
						<xsd:documentation>Kontrollimise aeg</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="Inspector" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>Kontrollija</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="Status">
					<xsd:annotation>
						<xsd:documentation>A- aktiivne (väärtus enam ei muutu). S - sisestamisel    (väärtus võib muutuda)</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="NavStatusType"/>
					</xsd:simpleType>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="EpisX18Type">
		<xsd:annotation>
			<xsd:documentation>X18 VÄÄRTPABERID</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="EpisMessageType">
				<xsd:choice>
					<xsd:element name="Request" type="EpisX18RequestType"/>
					<xsd:element name="Response" type="EpisX18ResponseType"/>
				</xsd:choice>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="EpisX18RequestType"/>
	<xsd:complexType name="EpisX18ResponseType">
		<xsd:sequence>
			<xsd:element name="Security" minOccurs="0" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:attribute name="ISIN" use="required">
						<xsd:annotation>
							<xsd:documentation>Väärtpaberi ISIN</xsd:documentation>
						</xsd:annotation>
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="12"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
					<xsd:attribute name="ShortName" use="required">
						<xsd:annotation>
							<xsd:documentation>Kuupäev, milleni soovitakse väljavõtet</xsd:documentation>
						</xsd:annotation>
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="7"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
					<xsd:attribute name="Name" use="required">
						<xsd:annotation>
							<xsd:documentation>Väärtpaberi nimetus</xsd:documentation>
						</xsd:annotation>
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="100"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
					<xsd:attribute name="Status" type="SecurityStatusType" use="required">
						<xsd:annotation>
							<xsd:documentation>Väärtpaberi staatus</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="Pillar" type="PillarType" use="required">
						<xsd:annotation>
							<xsd:documentation>Pillar</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
