<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2011 rel. 3 (x64) (http://www.altova.com) by Ildar Muhhamedjanov (Remarc Systems OÜ) -->
<xsd:schema xmlns="http://epis.x-road.ee/producer/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xml="http://www.w3.org/XML/1998/namespace" targetNamespace="http://epis.x-road.ee/producer/" elementFormDefault="qualified">
	<xsd:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="http://www.w3.org/2009/01/xml.xsd" />

	<!--============================================================================================================= -->
	<!--== DATA TYPES == -->
	<!--============================================================================================================= -->
	<xsd:complexType name="EpisMessageType">
		<xsd:sequence>
			<xsd:element name="Sender" minOccurs="0" maxOccurs="1">
				<xsd:complexType>
					<xsd:attribute name="CODE" type="xsd:string" use="required"/>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="ApplicationRequestType">
		<xsd:annotation>
			<xsd:documentation>Application request</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="PersonalData" type="PersonType" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation> ISIKUANDMED
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:choice>
				<xsd:element name="Address_ADS" type="AddressADSType" minOccurs="0" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation> AADRESS ADS
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="Address" type="AddressType" minOccurs="0" maxOccurs="1">
					<xsd:annotation>
						<xsd:documentation> AADRESS
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:choice>
		</xsd:sequence>
		<xsd:attribute name="DocumentDate" type="xsd:date" use="required"/>
		<xsd:attribute name="DocumentNumber">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:maxLength value="30"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="ApplicationRequest2Type">
		<xsd:complexContent>
			<xsd:extension base="ApplicationRequestType">
				<xsd:sequence>
					<xsd:element name="BankAccount" type="BankAccountType" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation>Pangakonto andmed</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="LegalRepresentative" type="LegalRepresentativeType" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation>Seaduslik esindaja</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Representative" type="RepresentativeType" minOccurs="0" maxOccurs="1">
						<xsd:annotation>
							<xsd:documentation>Esindaja</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ApplicationRequest2WithRedemptionUnitsType">
		<xsd:complexContent>
			<xsd:extension base="ApplicationRequest2Type">
				<xsd:sequence>
					<xsd:element name="ApplicationRows">
						<xsd:annotation>
							<xsd:documentation>Avalduse read</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="ApplicationRow" minOccurs="0" maxOccurs="unbounded">
									<xsd:annotation>
										<xsd:documentation>
										</xsd:documentation>
									</xsd:annotation>
									<xsd:complexType>
										<xsd:attribute name="ISIN" use="required">
											<xsd:annotation>
												<xsd:documentation>Väärtpaberi ISIN, mille osakuid soovitakse tagasi võtta</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="12"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:attribute>
										<xsd:attribute name="UnitAmount" type="xsd:decimal" use="required">
											<xsd:annotation>
												<xsd:documentation>Tagasivõetav osakute arv</xsd:documentation>
											</xsd:annotation>
										</xsd:attribute>
									</xsd:complexType>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ApplicationResponseType">
		<xsd:annotation>
			<xsd:documentation>Application response</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ResponseWithResultsType">
				<xsd:attribute name="PersonId" type="PersoncodeType">
					<xsd:annotation>
						<xsd:documentation>Isikukood</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="ApplicationId" type="xsd:integer">
					<xsd:annotation>
						<xsd:documentation>EVK poolt genereeritud id</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="PaymentAmount" type="xsd:decimal">
					<xsd:annotation>
						<xsd:documentation>EVK poolt vastavalt tariifidele selle teenuse eest kontohalduri käest küsitav tasu</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="Currency">
					<xsd:annotation>
						<xsd:documentation>vastavalt valuutadetabelile</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:length value="3"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ApplicationWithAddressResponseType">
		<xsd:annotation>
			<xsd:documentation>Application response</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ApplicationResponseType">
				<xsd:sequence minOccurs="0">
					<xsd:element name="Address" type="AddressType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>AADRESS
							</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:simpleType name="CountryType">
		<xsd:annotation>
			<xsd:documentation>ISO3166 A2</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string"/>
	</xsd:simpleType>
	<xsd:simpleType name="DistrictCodeType">
		<xsd:restriction base="xsd:string"/>
	</xsd:simpleType>
	<xsd:simpleType name="ApplicationTypeType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="FPAA">
				<xsd:annotation>
					<xsd:documentation>FPAA  - fondipension avamise avaldus</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="FPLA">
				<xsd:annotation>
					<xsd:documentation>FPLA  - Fondipension lõpetamise avaldus</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="FPMA">
				<xsd:annotation>
					<xsd:documentation>FPMA – Fondipensioni muutmise avaldus</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="MMTA">
				<xsd:annotation>
					<xsd:documentation>MMTA – maksemäära tõstmise avaldus</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="MATA">
				<xsd:annotation>
					<xsd:documentation>MATA – Maksete tegemise avaldus</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PAAV">
				<xsd:annotation>
					<xsd:documentation>PAAV - pärimisavaldus</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PEVA">
				<xsd:annotation>
					<xsd:documentation>PEVA – pensionifondi vahetamise avaldused</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PLAV">
				<xsd:annotation>
					<xsd:documentation>PLAV – Pensionilepingu avaldus</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="VAAV">
				<xsd:annotation>
					<xsd:documentation>VAAV - valikuavaldus</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="YKVA">
				<xsd:annotation>
					<xsd:documentation>YKVA  - ühekordse väljamakse avaldus</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="SUB">
				<xsd:annotation>
					<xsd:documentation>SUB – Valikuavaldus</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="RED">
				<xsd:annotation>
					<xsd:documentation>RED –Tagasivõtu avaldus</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="COR">
				<xsd:annotation>
					<xsd:documentation>COR - Kohtutäituri tagastamise avaldus</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="INS">
				<xsd:annotation>
					<xsd:documentation>INS – Kindlustusse kandmine</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="RES">
				<xsd:annotation>
					<xsd:documentation>RES – Ületoomise avaldus</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ApplicationStatusType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="I">
				<xsd:annotation>
					<xsd:documentation>I - sisestamisel</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="K">
				<xsd:annotation>
					<xsd:documentation>K - kinnitatud.  Investor on kinnitanud oma avalduse, paberil vastab sellele allkirjastatud avaldus</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="A">
				<xsd:annotation>
					<xsd:documentation>A - aktsepteeritud.  Kinnitatud avaldus oli korrektselt täidetud, EVK on selle vastu võtnud.  Mõningate avalduse liikide puhul jääb ta oma realiseerimist ootama, paberil vastab sellele märge vastuvõtmise</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="E">
				<xsd:annotation>
					<xsd:documentation>E- avaldus  ei täidetud.  Realiseerimise käigus selgus, et avaldust ei saanud täita. Reeglina põhjuseks mingite tingimuste muutus süsteemis.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="M">
				<xsd:annotation>
					<xsd:documentation>M -Maksmisel</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="P">
				<xsd:annotation>
					<xsd:documentation>P- protsess on käimas, avaldust menetletakse.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="R">
				<xsd:annotation>
					<xsd:documentation>R- realiseerutud.  Avaldus on täidetud.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="S">
				<xsd:annotation>
					<xsd:documentation>S - sisestatud. Osaliselt või lõplikult sisestatud avaldus, paberil vastab sellele allkirjastamata avaldus. Sellise staatusega avaldused töö käigus ignoreeritakse.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="T">
				<xsd:annotation>
					<xsd:documentation>T  - tühistatud.  Aktsepteeritud avaldus tühistati enne realiseerumist teise avalduse poolt.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="V">
				<xsd:annotation>
					<xsd:documentation>V -vigane. Kinnitatud avaldus oli täidetud vigaselt.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="LangType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="EST">
				<xsd:annotation>
					<xsd:documentation> EST
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="RUS">
				<xsd:annotation>
					<xsd:documentation> RUS
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="ENG">
				<xsd:annotation>
					<xsd:documentation> ENG
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="YesNoType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="N">
				<xsd:annotation>
					<xsd:documentation> Ei
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Y">
				<xsd:annotation>
					<xsd:documentation> Jah
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="">
				<xsd:annotation>
					<xsd:documentation>NULL</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="MailType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="E">
				<xsd:annotation>
					<xsd:documentation> E-post
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="P">
				<xsd:annotation>
					<xsd:documentation> Postiga
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="FrequencyType">
		<xsd:restriction base="xsd:int">
			<xsd:enumeration value="1">
				<xsd:annotation>
					<xsd:documentation> aasta
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="4">
				<xsd:annotation>
					<xsd:documentation> iga kvartal
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="12">
				<xsd:annotation>
					<xsd:documentation> iga kuu
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="PensionContractApplicationSubTypeType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="L1">
				<xsd:annotation>
					<xsd:documentation xml:lang="et">ühe isiku liitumine</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="LK">
				<xsd:annotation>
					<xsd:documentation xml:lang="et">ühine avaldus kindlustatud isikuga</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="K">
				<xsd:annotation>
					<xsd:documentation xml:lang="et">katkestada eelnevalt saadetud kindlustuslepingu avaldus (saab teha ainult 2 nädala jooksul) = taganemine</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="T">
				<xsd:annotation>
					<xsd:documentation xml:lang="et">täiendav makse</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="U">
				<xsd:annotation>
					<xsd:documentation xml:lang="et">pensionilepingu ülesütlemine</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="TA">
				<xsd:annotation>
					<xsd:documentation xml:lang="et">täiendav makse (automaatne)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="PersonType">
		<xsd:attribute name="PersonId" type="PersoncodeType" use="required">
			<xsd:annotation>
				<xsd:documentation> Kui algus=99 siis on pensionikonto muidu isikukood. Kood 00000000000 tähendab et tegemist on uue välismaalasega.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="BirthDate" type="xsd:date">
			<xsd:annotation>
				<xsd:documentation> Sünnikuupäev. Peab olema täidetud kui uus välismaalane  (PersonId=0000000000000). Muidu puudub.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="FirstName" use="required">
			<xsd:simpleType>
				<xsd:annotation>
					<xsd:documentation> Eesnimi
					</xsd:documentation>
				</xsd:annotation>
				<xsd:restriction base="xsd:string">
					<xsd:maxLength value="50"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="Name" use="required">
			<xsd:simpleType>
				<xsd:annotation>
					<xsd:documentation> Perekonnanimi
					</xsd:documentation>
				</xsd:annotation>
				<xsd:restriction base="xsd:string">
					<xsd:maxLength value="50"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="LanguagePreference" type="LangType" use="required">
			<xsd:annotation>
				<xsd:documentation> Keele eelistus
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="ExtractFlag" type="YesNoType" use="required">
			<xsd:annotation>
				<xsd:documentation> Väljavõtte soov Kas saata kord aasta pensionikonto väljavõte. Lubatud väärtused: Y - on soov N - mitte saata
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="ContactPreference" type="MailType">
			<xsd:annotation>
				<xsd:documentation> Lubatud E (e-post), P (postiga)
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="E_MAIL">
			<xsd:simpleType>
				<xsd:annotation>
					<xsd:documentation> E-post
					</xsd:documentation>
				</xsd:annotation>
				<xsd:restriction base="xsd:string">
					<xsd:maxLength value="240"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="Phone">
			<xsd:simpleType>
				<xsd:annotation>
					<xsd:documentation> Kontakttelefon
					</xsd:documentation>
				</xsd:annotation>
				<xsd:restriction base="xsd:string">
					<xsd:maxLength value="15"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="AddressADSType">
		<xsd:attribute name="ADS_OID" use="required">
			<xsd:annotation>
				<xsd:documentation>
				</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:maxLength value="20"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="Apartment">
			<xsd:annotation>
				<xsd:documentation> Korteri number. Täidetakse Maa-Ameti liidese poolt kui ei ole Maa-Ameti ametibaasis sellist korterit
				</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:maxLength value="20"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="PostalIndex" use="required">
			<xsd:annotation>
				<xsd:documentation> Sihtnumber
				</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:maxLength value="15"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="AddressType">
		<xsd:attribute name="Country" type="CountryType">
			<xsd:annotation>
				<xsd:documentation> peab esinema klassifikaatoris (ISO3166 A2)
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="Territory" type="DistrictCodeType">
			<xsd:annotation>
				<xsd:documentation> Haldusüksuse kood kui RIIK= EE,siis peab esinema klassifikaatoris, muidu peab puuduma
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="AddressRow1">
			<xsd:annotation>
				<xsd:documentation> Kui ADS_OID on tühi siis tänav, maja- ja korterinumber, väljaspool linnu, aleveid ja alevikke talu nimi
				</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:maxLength value="200"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="AddressRow2">
			<xsd:annotation>
				<xsd:documentation> Kui ADS_OID on tühi siis küla ja valla nimi
				</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:maxLength value="50"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="AddressRow3">
			<xsd:annotation>
				<xsd:documentation> Kui Kui ADS_OID on tühi ja LIIK=E siis tühi Kui LIIK =V ja ei ole Eesti riik siis linn või maakond
				</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:maxLength value="50"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="PostalIndex">
			<xsd:annotation>
				<xsd:documentation> Sihtnumber
				</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:maxLength value="15"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:simpleType name="BankAccountTypeType">
		<xsd:annotation>
			<xsd:documentation> Liik
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="E">
				<xsd:annotation>
					<xsd:documentation> Eesti
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="I">
				<xsd:annotation>
					<xsd:documentation> I- IBAN (Euroopa puhul)
					</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="V">
				<xsd:annotation>
					<xsd:documentation> Vaba
