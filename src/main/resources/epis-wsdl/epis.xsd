									</xsd:annotation>
								</xsd:element>
								<xsd:element name="InsuranceApplication" type="InsuranceApplicationType">
									<xsd:annotation>
										<xsd:documentation>Kindlustusse kandmine, ApplicationType = INS</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:choice>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ApplicationType">
		<xsd:sequence>
			<xsd:element name="ApplicationData">
				<xsd:annotation>
					<xsd:documentation>Tehnilised andmed</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:attribute name="ApplicationType" type="ApplicationTypeType" use="required">
						<xsd:annotation>
							<xsd:documentation>Application Type</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="DocumentId" use="required">
						<xsd:annotation>
							<xsd:documentation>EVK poolt antud dokumendi number</xsd:documentation>
						</xsd:annotation>
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="10"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
					<xsd:attribute name="Status" type="ApplicationStatusType" use="required">
						<xsd:annotation>
							<xsd:documentation>Staatus</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="PaymentAmount" type="xsd:decimal">
						<xsd:annotation>
							<xsd:documentation>Maksumus. Summa, mis laekub EVK-le selle avalduse esitamise ja töötlemise eest</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="Currency">
						<xsd:annotation>
							<xsd:documentation>Valuuta kood</xsd:documentation>
						</xsd:annotation>
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="3"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
					<xsd:attribute name="RegistryCode" type="RegistryCodeType">
						<xsd:annotation>
							<xsd:documentation>Juriidilise  isiku registrikood, kus avaldus
tehti. ”Minu konto”  puhul puudub</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="DocumentNumber">
						<xsd:annotation>
							<xsd:documentation>Alusdokumendi number</xsd:documentation>
						</xsd:annotation>
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:maxLength value="30"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
					<xsd:attribute name="DocumentDate" type="xsd:date" use="required">
						<xsd:annotation>
							<xsd:documentation>Dokumendi kuupäev</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="PersonalData" type="PersonType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Puudub kui ApplicationType=PAAV, muidu kohustusli</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Address" type="AddressType" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Aadress. Pärimisavalduse puhul pärija aadress.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BankAccount" type="BankAccountType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Pangakonto andmed. Pärimisavalduse puhul pärija pangakonto andmed.	</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LegalRepresentative" type="LegalRepresentativeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation> SEADUSLIK_ESINDAJA
							</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Representative" type="RepresentativeType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation> ESINDAJA
							</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SelectionApplicationType">
		<xsd:annotation>
			<xsd:documentation>Valikuavaldus ApplicationType= VAAV</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ApplicationType">
				<xsd:attribute name="ISIN" type="IsinType" use="required">
					<xsd:annotation>
						<xsd:documentation>Väärtpaberi ISIN</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ExchangeApplicationType">
		<xsd:annotation>
			<xsd:documentation>Osakute vahetamise avaldus.
ApplicationType=PEVA</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ApplicationType">
				<xsd:sequence>
					<xsd:element name="ExchangeApplicationRows" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Avalduse read. Ridade puudumine  näitab et olemasolevaid osakuid ei vahetata  (eelmiste avalduste tühistamine)</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="ExchangeApplicationRow" minOccurs="0" maxOccurs="unbounded">
									<xsd:complexType>
										<xsd:attribute name="DestinationISIN" type="IsinType" use="required">
											<xsd:annotation>
												<xsd:documentation>Väärtpaberi ISIN, mille osakud lastakse välja</xsd:documentation>
											</xsd:annotation>
										</xsd:attribute>
										<xsd:attribute name="Percentage" use="required">
											<xsd:annotation>
												<xsd:documentation>Milline osa lunastatud osakutest  kasutatakse sihtfondi osakute märkimiseks</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="xsd:decimal">
													<xsd:maxInclusive value="100"/>
													<xsd:minInclusive value="0"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:attribute>
									</xsd:complexType>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
				<xsd:attribute name="Pillar" type="PillarType" use="required">
					<xsd:annotation>
						<xsd:documentation>Sammas . Väärtus 2 või 3</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="SourceISIN" type="IsinType" use="required">
					<xsd:annotation>
						<xsd:documentation>Väärtpaberi ISIN, mille osakud lunastatakse</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="FundPensionOpenType">
		<xsd:annotation>
			<xsd:documentation>Fondipensioni avamise avaldus
ApplicationType= FPAA</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ApplicationType">
				<xsd:attribute name="Duration">
					<xsd:annotation>
						<xsd:documentation>Isiku poolt soovitud maksete kestus. Ei või olla lühem, kui määruses määratud. Vaata määrus § 2 ja 3 Tühi väärtus tähendab minimaalset kestust</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:int">
							<xsd:maxInclusive value="99"/>
							<xsd:minInclusive value="0"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
				<xsd:attribute name="Frequency" type="FrequencyType" use="required">
					<xsd:annotation>
						<xsd:documentation>Väljamaksete tegemise sagedus. Lubatud väärtused
12 -iga kuu
4- iga kvartal
1 - aasta</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="LimitFlag" type="YesNoType">
					<xsd:annotation>
						<xsd:documentation>Soov suurendada arvestuslikku osakute arvu minimaalse piirsummani
Lubatud väärtused  Y,N</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="FundPensionChangeType">
		<xsd:annotation>
			<xsd:documentation>Fondipensioni muutmise avaldus 
ApplicationType= FPMA</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ApplicationType">
				<xsd:attribute name="Frequency" type="FrequencyType" use="required">
					<xsd:annotation>
						<xsd:documentation>Väljamaksete tegemise sagedus. Lubatud väärtused
12 -iga kuu
4- iga kvartal
1 - aasta</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="LimitFlag" type="YesNoType">
					<xsd:annotation>
						<xsd:documentation>Soov suurendada arvestuslikku osakute arvu minimaalse piirsummani
Lubatud väärtused  Y,N</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="PensionContractApplicationType">
		<xsd:annotation>
			<xsd:documentation>Pensionilepingu avaldus
ApplicationType= PLAV</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ApplicationType">
				<xsd:sequence>
					<xsd:element name="InsurancePayment" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation> Kindlustusmakse. Võib kasutada kui SubType=L1 või LK
										</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:attribute name="OldInsuranceComapany" use="required">
								<xsd:annotation>
									<xsd:documentation> Kindlustusseltsi registrikood, mille raha kanti üle
												</xsd:documentation>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:maxLength value="8"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:attribute>
							<xsd:attribute name="OldContractNumber" use="required">
								<xsd:annotation>
									<xsd:documentation> Üleaöeldud lepingu Number
												</xsd:documentation>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:maxLength value="30"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:attribute>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="InsuredPerson" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation> Ainult siis kui SubType=LK. Siis kohustuslik
										</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:attribute name="InsuredPersonId">
								<xsd:annotation>
									<xsd:documentation> Kindlustatud isiku isikukood Võib välismaalasel puududa
												</xsd:documentation>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:maxLength value="30"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:attribute>
							<xsd:attribute name="InsuredPersonFirstName" use="required">
								<xsd:annotation>
									<xsd:documentation> Kindlustatud isiku eesnimi
												</xsd:documentation>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:maxLength value="50"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:attribute>
							<xsd:attribute name="InsuredPersonName" use="required">
								<xsd:annotation>
									<xsd:documentation> Kindlustatud isiku perekonnanimi
												</xsd:documentation>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:restriction base="xsd:string">
										<xsd:maxLength value="50"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:attribute>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="Cancel" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation> Ainult siis kui SubType=U. Siis kohustuslik
										</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:attribute name="CloseDate" type="xsd:date" use="required">
								<xsd:annotation>
									<xsd:documentation> Pensionilepingu lõpetamise kuuüpäev
												</xsd:documentation>
								</xsd:annotation>
							</xsd:attribute>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="ApplicationRows" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Avalduse read</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element name="ApplicationRow" minOccurs="0" maxOccurs="unbounded">
									<xsd:annotation>
										<xsd:documentation> Avalduse read
										</xsd:documentation>
									</xsd:annotation>
									<xsd:complexType>
										<xsd:attribute name="ISIN" use="required">
											<xsd:annotation>
												<xsd:documentation> Väärtpaberi ISIN, mille osakute eest on vaja raha saata kindlustusseltsile
												</xsd:documentation>
											</xsd:annotation>
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:maxLength value="12"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:attribute>
										<xsd:attribute name="RowAllUnitsFlag" type="YesNoType" use="required">
											<xsd:annotation>
												<xsd:documentation> Tunnus kas kõik osakud üle kanda. Lubatud väärtused N (ei), Y (kõik osakud üle kanda)
												</xsd:documentation>
											</xsd:annotation>
										</xsd:attribute>
										<xsd:attribute name="Amount" type="xsd:decimal" use="required">
											<xsd:annotation>
												<xsd:documentation> Avaldusel olev osakute arv, mida tagasi võtta. Kui RowAllUnitsFlag =Y, siis peab olema NULL
												</xsd:documentation>
											</xsd:annotation>
										</xsd:attribute>
									</xsd:complexType>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
				<xsd:attribute name="InsuranceCompany" type="RegistryCodeType" use="required">
					<xsd:annotation>
						<xsd:documentation>Kindlustusseltsi, kust avaldus saadeti, registrikood</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="SubType" type="PensionContractApplicationSubTypeType" use="required">
					<xsd:annotation>
						<xsd:documentation>Avalduse sisu, täpsustus mida teha</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="ContractNumber" use="required">
					<xsd:annotation>
						<xsd:documentation>Pensionilepingu number</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:maxLength value="30"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
				<xsd:attribute name="ReferenceNumber" use="required">
					<xsd:annotation>
						<xsd:documentation>Viitenumber</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:maxLength value="20"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
				<xsd:attribute name="AllUnitsFlag" type="YesNoType" use="required">
					<xsd:annotation>
						<xsd:documentation>Tunnus kas kõik osakud üle kanda.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="TopUp" type="YesNoType">
					<xsd:annotation>
						<xsd:documentation>Y  kui avalduse tegemisel oli saldo top-up tunnustega</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="TransferFlag" type="YesNoType">
					<xsd:annotation>
						<xsd:documentation>Tunnus, et  kindlustusmakseks on ülesöeldud pensionilepingu tagastusväärtus Lubatud väärtus</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="Comments">
					<xsd:annotation>
						<xsd:documentation>Kommentaarid</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:maxLength value="50"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="InheritanceApplicationType">
		<xsd:annotation>
			<xsd:documentation>Pärimisavaldus
ApplicationType= PAAV</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ApplicationType">
				<xsd:sequence>
					<xsd:element name="Bequeather">
						<xsd:annotation>
							<xsd:documentation> Pärandaja andmed
					</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:attribute name="PersonId" use="required">
								<xsd:annotation>
									<xsd:documentation>Eesti isikukood</xsd:documentation>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:annotation>
										<xsd:documentation> Eesti isikukood
								</xsd:documentation>
									</xsd:annotation>
									<xsd:restriction base="xsd:string">
										<xsd:maxLength value="11"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:attribute>
							<xsd:attribute name="FirstName" use="required">
								<xsd:annotation>
									<xsd:documentation>Pärandaja eesnimi</xsd:documentation>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:annotation>
										<xsd:documentation> Pärandaja eesnimi
								</xsd:documentation>
									</xsd:annotation>
									<xsd:restriction base="xsd:string">
										<xsd:maxLength value="50"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:attribute>
							<xsd:attribute name="Name" use="required">
								<xsd:annotation>
									<xsd:documentation>Pärandaja perekonnanimi</xsd:documentation>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:annotation>
										<xsd:documentation> Pärandaja perekonnanimi
								</xsd:documentation>
									</xsd:annotation>
									<xsd:restriction base="xsd:string">
										<xsd:maxLength value="50"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:attribute>
							<xsd:attribute name="DeathDate" type="xsd:date">
								<xsd:annotation>
									<xsd:documentation>Surma kuupäev. Täidetakse kui isik on surnud</xsd:documentation>
								</xsd:annotation>
							</xsd:attribute>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="IndividualHeir" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation> Kohustuslik element, kui pärijaks on füüsiline isik. Peab puuduma, kui pärijaks on juriidiline isik
					</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:attribute name="CountryCode" type="CountryType" use="required">
								<xsd:annotation>
									<xsd:documentation> ISO3166 2-kohaline kood
							</xsd:documentation>
								</xsd:annotation>
							</xsd:attribute>
							<xsd:attribute name="Code" use="required">
								<xsd:annotation>
									<xsd:documentation>Eesti puhul isikukood.  Välismaalaste puhul nende identifitseerimise kood või sünniaeg</xsd:documentation>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:annotation>
										<xsd:documentation> Eesti puhul isikukood. Välismaalaste puhul nende identifitseerimise kood või sünniaeg
								</xsd:documentation>
									</xsd:annotation>
									<xsd:restriction base="xsd:string">
										<xsd:maxLength value="11"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:attribute>
							<xsd:attribute name="FirstName" use="required">
								<xsd:annotation>
									<xsd:documentation>Eesnimi</xsd:documentation>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:annotation>
										<xsd:documentation> Eesnimi
								</xsd:documentation>
									</xsd:annotation>
									<xsd:restriction base="xsd:string">
										<xsd:maxLength value="50"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:attribute>
							<xsd:attribute name="Name" use="required">
								<xsd:annotation>
									<xsd:documentation>Perekonnanimi</xsd:documentation>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:annotation>
										<xsd:documentation> Perekonnanimi
								</xsd:documentation>
									</xsd:annotation>
									<xsd:restriction base="xsd:string">
										<xsd:maxLength value="50"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:attribute>
							<xsd:attribute name="E_MAIL" use="required">
								<xsd:annotation>
									<xsd:documentation>peab olema kui KONTAKTI EELISTUS=E</xsd:documentation>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:annotation>
										<xsd:documentation> E-post. Peab olema kui KONTAKTI EELISTUS=E
								</xsd:documentation>
									</xsd:annotation>
									<xsd:restriction base="xsd:string">
										<xsd:maxLength value="240"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:attribute>
							<xsd:attribute name="Phone">
								<xsd:simpleType>
									<xsd:annotation>
										<xsd:documentation> Kontakttelefon
								</xsd:documentation>
									</xsd:annotation>
									<xsd:restriction base="xsd:string">
										<xsd:maxLength value="15"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:attribute>
						</xsd:complexType>
					</xsd:element>
					<xsd:element name="LegalPersonHeir" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation> Kohustuslik element, kui pärijaks on juriidiline isik. Peab puuduma, kui pärijaks on füüsiline isik
					</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:attribute name="CountryCode" type="CountryType" use="required">
								<xsd:annotation>
									<xsd:documentation> ISO3166 2-kohaline kood
							</xsd:documentation>
								</xsd:annotation>
							</xsd:attribute>
							<xsd:attribute name="Code" use="required">
								<xsd:annotation>
									<xsd:documentation>Eesti puhul registrikood</xsd:documentation>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:annotation>
										<xsd:documentation> Eesti puhul registrikood
								</xsd:documentation>
									</xsd:annotation>
									<xsd:restriction base="xsd:string">
										<xsd:maxLength value="11"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:attribute>
							<xsd:attribute name="Name" use="required">
								<xsd:annotation>
									<xsd:documentation>Juriidilise isiku nimetus</xsd:documentation>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:annotation>
